{
  "name": "view-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "watch": "run-p type-check \"watch-only {@}\" --",
    "watch-only": "vite build --watch",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "build:panel": "vue-tsc -b && vite build --outDir out/extension/panel",
    "build:sidebar": "vue-tsc -b && vite build --outDir out/extension/sidebar"
  },
  "dependencies": {
    "@types/node": "~22.18.5",
    "npm-run-all2": "^8.0.4",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
